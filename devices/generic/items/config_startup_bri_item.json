{
  "schema": "resourceitem1.schema.json",
  "id": "config/startup/bri",
  "datatype": "UInt8",
  "access": "RW",
  "public": true,
  "description": "Brightness on device startup (power on).",
  "default": 254,
  "parse": {
    "fn": "zcl", "ep": 0, "cl": "0x0008", "at": "0x4000",
    "eval": "Item.val = Attr.val"
  },
  "read": {
    "fn": "zcl", "ep": 0, "cl": "0x0008", "at": "0x4000"
  },
  "refresh.interval": 86400
}
